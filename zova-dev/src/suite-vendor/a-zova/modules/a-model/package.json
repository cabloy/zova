{
  "name": "zova-module-a-model",
  "version": "5.0.34",
  "title": "a-model",
  "zovaModule": {
    "capabilities": {
      "monkey": true
    },
    "globalDependencies": {
      "mutate-on-copy": true
    },
    "bundle": {
      "vendors": [
        {
          "match": [
            "@tanstack"
          ],
          "output": "tanstack"
        },
        {
          "match": [
            "localforage"
          ],
          "output": "localforage"
        }
      ]
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./*": "./*"
  },
  "description": "core",
  "keywords": [
    "Zova Module"
  ],
  "author": "zhennann",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "tsc -b --clean",
    "tsc:publish": "npm run clean && tsc -b",
    "_prepublishOnly": "npm run tsc:publish"
  },
  "dependencies": {
    "@tanstack/query-persist-client-core": "^5.45.0",
    "@tanstack/vue-query": "^5.45.0",
    "localforage": "^1.10.0",
    "mutate-on-copy": "^1.0.1"
  },
  "gitHead": "0b158ae33baa46cda98b12d496fba85d331236f4"
}
