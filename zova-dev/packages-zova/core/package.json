    "@cabloy/vue-runtime-core": "^3.4.35",
    "@types/js-cookie": "^3.0.6",
    "@vue/reactivity": "^3.4.27",
    "js-cookie": "^3.0.5",
    "reflect-metadata": "^0.2.2",
    "vue": "^3.4.27",
