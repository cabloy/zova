{
  "name": "zova-module-a-quasar",
  "version": "5.0.0",
  "title": "a-quasar",
  "zovaModule": {
    "capabilities": {
      "monkey": true
    },
    "dependencies": {
      "a-core": "5.0.0"
    },
    "bundle": {
      "vendors": [
        {
          "match": [
            "quasar"
          ],
          "output": "quasar"
        }
      ]
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./*": "./*"
  },
  "description": "quasar",
  "keywords": [
    "Zova Module"
  ],
  "author": "zhennann",
  "files": [
    "dist"
  ],
  "scripts": {},
  "dependencies": {}
}
