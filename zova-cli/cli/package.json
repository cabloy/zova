{
  "name": "zova-cli",
  "version": "1.1.208",
  "description": "zova cli",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "bin": {
    "zova": "dist/bin/zova.js"
  },
  "exports": {
    ".": {
      "types": [
        "./src/index.ts",
        "./dist/index.d.ts"
      ],
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "clean": "tsc -b --clean",
    "tsc:publish": "npm run clean && tsc -b",
    "prepublishOnly": "npm run tsc:publish"
  },
  "keywords": [
    "framework",
    "zova"
  ],
  "author": "zhennann",
  "license": "MIT",
  "dependencies": {
    "@cabloy/cli": "^2.0.2",
    "zova-cli-set-front": "^1.1.195"
  },
  "gitHead": "73721393c1ea9a95539948d57ad692f2c004d7c7"
}
